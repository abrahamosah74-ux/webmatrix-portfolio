<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Car Dealership website preview">
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="carGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ec4899;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f43f5e;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="600" height="400" fill="url(#bgGrad)" />

  <!-- Header Bar -->
  <rect width="600" height="60" fill="rgba(255,255,255,0.95)" />
  <text x="30" y="42" font-family="Arial, sans-serif" font-size="28" font-weight="bold" fill="#0f172a">Dealership</text>

  <!-- Car Shape -->
  <g transform="translate(150, 100)">
    <!-- Car body -->
    <ellipse cx="150" cy="120" rx="100" ry="50" fill="url(#carGrad)" />
    
    <!-- Car top -->
    <rect x="100" y="60" width="100" height="50" rx="20" fill="url(#carGrad)" />
    
    <!-- Windows -->
    <rect x="110" y="70" width="35" height="25" rx="5" fill="rgba(255,255,255,0.4)" />
    <rect x="155" y="70" width="35" height="25" rx="5" fill="rgba(255,255,255,0.4)" />
    
    <!-- Wheels -->
    <circle cx="85" cy="170" r="25" fill="#1e293b" />
    <circle cx="215" cy="170" r="25" fill="#1e293b" />
    <circle cx="85" cy="170" r="18" fill="#64748b" />
    <circle cx="215" cy="170" r="18" fill="#64748b" />
  </g>

  <!-- Inventory Grid -->
  <g transform="translate(320, 100)">
    <rect x="0" y="0" width="50" height="50" rx="8" fill="rgba(255,255,255,0.2)" />
    <rect x="60" y="0" width="50" height="50" rx="8" fill="rgba(255,255,255,0.2)" />
    <rect x="0" y="60" width="50" height="50" rx="8" fill="rgba(255,255,255,0.2)" />
    <rect x="60" y="60" width="50" height="50" rx="8" fill="rgba(255,255,255,0.2)" />
  </g>

  <!-- Features text -->
  <text x="30" y="280" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="white">Responsive Design</text>
  <text x="30" y="310" font-family="Arial, sans-serif" font-size="14" fill="rgba(255,255,255,0.9)">Modern inventory showcase with smooth animations</text>
  
  <text x="30" y="350" font-family="Arial, sans-serif" font-size="14" fill="rgba(255,255,255,0.8)">✓ Mobile-optimized  ✓ Fast loading  ✓ SEO ready</text>
</svg>
